            <child>
              <object class="GtkButton" id="cours_btn_edit"><property name="label">âœï¸ Modifier</property><property name="visible">True</property><signal name="clicked" handler="on_cours_edit"/></object>
            </child>
            <child>
              <object class="GtkButton" id="cours_btn_delete"><property name="label">ğŸ—‘ï¸ Supprimer</property><property name="visible">True</property><signal name="clicked" handler="on_cours_delete"/></object>
            </child>
            <child>
