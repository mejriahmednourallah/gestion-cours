<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.0"/>
  
  <!-- ============= LOGIN WINDOW ============= -->
  <object class="GtkWindow" id="login_window">
    <property name="type">toplevel</property>
    <property name="modal">True</property>
    <property name="window-position">center</property>
    <property name="default-width">400</property>
    <property name="default-height">300</property>
    <property name="title">Authentification - Gestion des Centres Sportifs</property>
    <signal name="destroy" handler="on_login_window_destroy"/>
    <child>
      <object class="GtkBox" id="login_box">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="orientation">vertical</property>
        <property name="margin-top">20</property>
        <property name="margin-bottom">20</property>
        <property name="margin-start">20</property>
        <property name="margin-end">20</property>
        <property name="spacing">15</property>
        <child>
          <object class="GtkLabel" id="login_title">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="label">Gestion des Centres Sportifs</property>
            <attributes>
              <attribute name="size" value="18000"/>
              <attribute name="weight" value="bold"/>
            </attributes>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="login_subtitle">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="label">Veuillez sélectionner votre rôle</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        
        <!-- Role Selection -->
        <child>
          <object class="GtkBox" id="role_box">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">10</property>
            <child>
              <object class="GtkLabel" id="role_label">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="label">Rôle:</property>
                <property name="width-request">100</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkComboBoxText" id="role_combo">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="active">0</property>
                <items>Administrateur
Membre
Entraîneur</items>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">2</property>
          </packing>
        </child>
        
        <!-- Username -->
        <child>
          <object class="GtkBox" id="username_box">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">10</property>
            <child>
              <object class="GtkLabel" id="username_label">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="label">Utilisateur:</property>
                <property name="width-request">100</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkEntry" id="username_entry">
                <property name="visible">True</property>
                <property name="can-focus">True</property>
                <property name="placeholder-text">Entrez votre nom d'utilisateur</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">3</property>
          </packing>
        </child>
        
        <!-- Password -->
        <child>
          <object class="GtkBox" id="password_box">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">10</property>
            <child>
              <object class="GtkLabel" id="password_label">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="label">Mot de passe:</property>
                <property name="width-request">100</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkEntry" id="password_entry">
                <property name="visible">True</property>
                <property name="can-focus">True</property>
                <property name="placeholder-text">Entrez votre mot de passe</property>
                <property name="visibility">False</property>
                <property name="input-purpose">password</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">4</property>
          </packing>
        </child>
        
        <!-- Info Message -->
        <child>
          <object class="GtkLabel" id="login_info">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="label">Admin: user/pass, Membre: member/member, Entraîneur: trainer/trainer</property>
            <property name="wrap">True</property>
            <attributes>
              <attribute name="style" value="italic"/>
              <attribute name="size" value="8000"/>
            </attributes>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">5</property>
          </packing>
        </child>
        
        <!-- Buttons -->
        <child>
          <object class="GtkBox" id="login_buttons">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">10</property>
            <property name="margin-top">20</property>
            <child>
              <object class="GtkButton" id="login_button">
                <property name="label">Se Connecter</property>
                <property name="visible">True</property>
                <property name="can-focus">True</property>
                <property name="can-default">True</property>
                <property name="receives-default">True</property>
                <signal name="clicked" handler="on_login_button_clicked"/>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="cancel_button">
                <property name="label">Quitter</property>
                <property name="visible">True</property>
                <property name="can-focus">True</property>
                <property name="receives-default">True</property>
                <signal name="clicked" handler="on_cancel_button_clicked"/>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
            <property name="position">6</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  
  <!-- ============= MAIN WINDOW ============= -->
  <object class="GtkWindow" id="main_window">
    <property name="type">toplevel</property>
    <property name="window-position">center</property>
    <property name="default-width">1200</property>
    <property name="default-height">700</property>
    <property name="title">Gestion des Centres Sportifs</property>
    <signal name="destroy" handler="on_main_window_destroy"/>
    <child>
      <object class="GtkBox" id="main_box">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkMenuBar" id="menubar">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <child>
              <object class="GtkMenuItem" id="file_menu">
                <property name="label">Fichier</property>
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <child type="submenu">
                  <object class="GtkMenu" id="file_submenu">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <child>
                      <object class="GtkMenuItem" id="logout_menu">
                        <property name="label">Déconnexion</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_logout_menu"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkSeparatorMenuItem" id="separator1">
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="quit_menu">
                        <property name="label">Quitter</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_quit_menu"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="modules_menu">
                <property name="label">Modules</property>
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <child type="submenu">
                  <object class="GtkMenu" id="modules_submenu">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <child>
                      <object class="GtkMenuItem" id="menu_membres">
                        <property name="label">Membres</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_menu_membres"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="menu_centres">
                        <property name="label">Centres</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_menu_centres"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="menu_courses">
                        <property name="label">Cours</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_menu_courses"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="menu_trainers">
                        <property name="label">Entraîneurs</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_menu_trainers"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="menu_equipment">
                        <property name="label">Équipements</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_menu_equipment"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="menu_events">
                        <property name="label">Événements</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_menu_events"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="help_menu">
                <property name="label">Aide</property>
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <child type="submenu">
                  <object class="GtkMenu" id="help_submenu">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <child>
                      <object class="GtkMenuItem" id="about_menu">
                        <property name="label">À propos</property>
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <signal name="activate" handler="on_about_menu"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">0</property>
          </packing>
        </child>
        
        <!-- Header Bar -->
        <child>
          <object class="GtkBox" id="header_box">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="orientation">horizontal</property>
            <property name="margin-top">10</property>
            <property name="margin-bottom">10</property>
            <property name="margin-start">10</property>
            <property name="margin-end">10</property>
            <property name="spacing">10</property>
            <child>
              <object class="GtkLabel" id="current_user_label">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="label">Utilisateur: Admin</property>
                <attributes>
                  <attribute name="weight" value="bold"/>
                </attributes>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="current_role_label">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="label">Rôle: Administrateur</property>
                <attributes>
                  <attribute name="weight" value="bold"/>
                </attributes>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        
        <!-- Content Notebook -->
        <child>
          <object class="GtkNotebook" id="content_notebook">
            <property name="visible">True</property>
            <property name="can-focus">True</property>
            <property name="margin-top">5</property>
            <property name="margin-bottom">5</property>
            <property name="margin-start">5</property>
            <property name="margin-end">5</property>
            
            <!-- Welcome Tab -->
            <child>
              <object class="GtkBox" id="welcome_tab">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="orientation">vertical</property>
                <property name="margin">20</property>
                <property name="spacing">20</property>
                <child>
                  <object class="GtkLabel" id="welcome_title">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="label">Bienvenue!</property>
                    <attributes>
                      <attribute name="size" value="28000"/>
                      <attribute name="weight" value="bold"/>
                    </attributes>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">0</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="welcome_text">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="label">Application de gestion des centres sportifs

Utilisez le menu "Modules" pour accéder aux différentes sections de l'application:
• Membres - Gérer les adhérents
• Centres - Gérer les centres sportifs
• Cours - Gérer les cours sportifs
• Entraîneurs - Gérer le personnel
• Équipements - Gérer l'inventaire
• Événements - Gérer les événements spéciaux</property>
                    <property name="wrap">True</property>
                    <property name="justify">center</property>
                  </object>
                  <packing>
                    <property name="expand">True</property>
                    <property name="fill">True</property>
                    <property name="position">1</property>
                  </packing>
                </child>
              </object>
            </child>
            <child type="tab">
              <object class="GtkLabel" id="welcome_tab_label">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="label">Accueil</property>
              </object>
              <packing>
                <property name="tab-fill">False</property>
              </packing>
            </child>
            
            <!-- Membres Tab -->
            <child>
              <object class="GtkBox" id="membres_tab">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <property name="margin">10</property>
                
                <!-- Search Box -->
                <child>
                  <object class="GtkBox" id="membres_search_box">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="orientation">horizontal</property>
                    <property name="spacing">5</property>
                    <child>
                      <object class="GtkLabel" id="membres_search_label">
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <property name="label">Rechercher:</property>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">0</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkSearchEntry" id="membres_search_entry">
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <property name="placeholder-text">Rechercher par nom, email ou ID...</property>
                      </object>
                      <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                        <property name="position">1</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="membres_search_button">
                        <property name="label">Rechercher</property>
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <property name="receives-default">True</property>
                        <signal name="clicked" handler="on_membres_search"/>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">2</property>
                      </packing>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">0</property>
                  </packing>
                </child>
                
                <!-- TreeView for Membres -->
                <child>
                  <object class="GtkScrolledWindow" id="membres_scrolled">
                    <property name="visible">True</property>
                    <property name="can-focus">True</property>
                    <property name="shadow-type">in</property>
                    <child>
                      <object class="GtkTreeView" id="membres_treeview">
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <child internal-child="selection">
                          <object class="GtkTreeSelection" id="membres_selection"/>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="col_id">
                              <property name="title">ID</property>
                              <child>
                                <object class="GtkCellRendererText" id="cell_id"/>
                                <attributes>
                                  <attribute name="text">0</attribute>
                                </attributes>
                              </child>
                            </object>
                          </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="col_nom">
                              <property name="title">Nom</property>
                              <child>
                                <object class="GtkCellRendererText" id="cell_nom"/>
                                <attributes>
                                  <attribute name="text">1</attribute>
                                </attributes>
                              </child>
                            </object>
                          </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="col_prenom">
                              <property name="title">Prénom</property>
                              <child>
                                <object class="GtkCellRendererText" id="cell_prenom"/>
                                <attributes>
                                  <attribute name="text">2</attribute>
                                </attributes>
                              </child>
                            </object>
                          </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="col_email">
                              <property name="title">Email</property>
                              <child>
                                <object class="GtkCellRendererText" id="cell_email"/>
                                <attributes>
                                  <attribute name="text">3</attribute>
                                </attributes>
                              </child>
                            </object>
                          </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="col_telephone">
                              <property name="title">Téléphone</property>
                              <child>
                                <object class="GtkCellRendererText" id="cell_telephone"/>
                                <attributes>
                                  <attribute name="text">4</attribute>
                                </attributes>
                              </child>
                            </object>
                          </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="col_date">
                              <property name="title">Inscription</property>
                              <child>
                                <object class="GtkCellRendererText" id="cell_date"/>
                                <attributes>
                                  <attribute name="text">5</attribute>
                                </attributes>
                              </child>
                            </object>
                          </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="col_actif">
                              <property name="title">Actif</property>
                              <child>
                                <object class="GtkCellRendererText" id="cell_actif"/>
                                <attributes>
                                  <attribute name="text">6</attribute>
                                </attributes>
                              </child>
                            </object>
                          </child>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">True</property>
                    <property name="fill">True</property>
                    <property name="position">1</property>
                  </packing>
                </child>
                
                <!-- Action Buttons -->
                <child>
                  <object class="GtkBox" id="membres_button_box">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="orientation">horizontal</property>
                    <property name="spacing">5</property>
                    <child>
                      <object class="GtkButton" id="membres_add_button">
                        <property name="label">Ajouter</property>
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <property name="receives-default">True</property>
                        <signal name="clicked" handler="on_membres_add"/>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">0</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="membres_edit_button">
                        <property name="label">Modifier</property>
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <property name="receives-default">True</property>
                        <signal name="clicked" handler="on_membres_edit"/>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">1</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkButton" id="membres_delete_button">
                        <property name="label">Supprimer</property>
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <property name="receives-default">True</property>
                        <signal name="clicked" handler="on_membres_delete"/>
                      </object>
                      <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">2</property>
                      </packing>
                    </child>
