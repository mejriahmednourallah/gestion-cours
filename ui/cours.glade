            </child>
            <child>
              <object class="GtkButton" id="cours_btn_join_trainer"><property name="label">ğŸ‘¨â€ğŸ« Rejoindre</property><property name="visible">False</property><signal name="clicked" handler="on_cours_join_as_trainer"/></object>
